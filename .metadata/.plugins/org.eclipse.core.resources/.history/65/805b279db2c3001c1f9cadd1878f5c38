import java.net.InetAddress;
public class MainApp {
	public static void main(String[] args) {
		 try {
			 InetAddress addr
             = InetAddress.getByName("142.250.195.110");

         // Host name
         System.out.println("Host name is: "
                            + addr.getHostName());
         System.out.println("Host address is: "+addr.getHostAddress());
         System.out.println("Canonical host name is: "+addr.getCanonicalHostName());
         System.out.println("Address is: "+addr.getAddress());
         System.out.println("Local host is: "+addr.getLocalHost());
         
         // Host Address
         System.out.println("Ip address is: "
                            + addr.getHostAddress());
